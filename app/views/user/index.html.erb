<h1>Sign Up!</h1>

<%= form_for @user, url: {action: "create"} do |u| %>
  <%= u.label "username" %>
  <%= u.text_field :username %><br>

  <%= u.label "email" %>
  <%= u.text_field :email %><br>

  <%= u.label "password" %>
  <%= u.password_field :password %><br>

  <%= u.submit "Let's Party" %>
<% end %>
